{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "forceConsistentCasingInFileNames": true,
    "noUncheckedIndexedAccess": true,
    "strictNullChecks": true,
    "strict": true,
    "moduleResolution": "NodeNext",
    "noEmit": true,
  },
  "include": ["src"],
}
